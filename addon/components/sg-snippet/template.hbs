<div class='sg-snippet' data-test-id={{name}}>
  {{!-- {{#copy-button
    clipboardText=snippetText
    title='copy to clipboard'
  }}
    Copy
  {{/copy-button}} --}}

  {{!-- {{#if hasBlock}}
    {{code-snippet name=(concat name '.hbs')}}
  {{else}}
    {{code-snippet name=name language=language}}
  {{/if}} --}}

  <pre><code class='hljs'>{{{highlightedSnippet}}}</code></pre>
</div>
